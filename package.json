{
  "name": "make-cacheable",
  "version": "2.0.0",
  "author": "Rubén Norte <ruben.norte@softonic.com>",
  "contributors": [
    "Marc Mignonsin <marc.mignonsin@softonic.com>",
    "Natàlia Calvet i Vidal <natalia.calvet@softonic.com"
  ],
  "description": "Decorates functions to make them cacheable with Catbox",
  "license": "Apache-2.0",
  "main": "index.js",
  "homepage": "https://github.com/softonic/make-cacheable",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softonic/make-cacheable.git"
  },
  "files": [
    "es",
    "lib",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint es",
    "pretest": "npm run lint",
    "tdd": "jest --watch",
    "test": "jest --coverage",
    "prebuild": "npm run test",
    "build": "rm -rf lib && babel es -d lib --source-maps",
    "prerelease": "npm run build",
    "release": "npm version -m \"New version: %s\"",
    "postrelease": "npm run push && npm publish",
    "push": "git push origin master && git push origin --tags"
  },
  "dependencies": {
    "catbox": "^10.0.5",
    "ms": "^0.7.1",
    "object-hash": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "catbox-memory": "^4.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "jest": "^23.6.0"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*\\.spec\\.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/coverage/",
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "es/**/*.js"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/.*(?!\\.spec\\.js)"
    ]
  }
}
